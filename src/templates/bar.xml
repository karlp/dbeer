<?xml version="1.0" encoding="UTF-8"?>
<bar lat="{{bar.location.lat}}" lon="{{bar.location.lon}}" osmid="{{bar.bar_osm_id}}">
    <name>{{bar.name}}</name>
    <type>{{bar.type}}</type>
    <prices>
        {%- for price in prices -%}
        <price drinkid="{{price}}">{{prices[price]}}</price>
        {%- endfor -%}
    </prices>
</bar>